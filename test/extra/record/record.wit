record bar {
    name: string,
    age: s32
}

record deep-bar {
  id: s32,
  x: bar
}

record deeper-bar {
  id: s32,
  x: deep-bar
}

record foo {
  movies: list<string>,
  code: list<s32>,
  bars: list<bar>,
  b: list<bool>
}

construct-bar: function(name: string, age: s32) -> bar
test-record: function(a: bar) -> s32
test-deep-record: function(a: deep-bar) -> s32
test-deeper-record: function(a: deeper-bar) -> s32
bar: function(a: bar) -> bar
deep-bar: function(a: bar) -> deep-bar
deeper-bar: function(a: bar) -> deeper-bar
rev-deeper-bar: function(a: deeper-bar) -> deeper-bar

foo: function() -> foo
test-foo: function(a: foo) -> foo
